{
  "name": "express-mariadb-news",
  "version": "1.0.0",
  "description": "express-mariadb-news",
  "main": "server.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "start": "nodemon ./bin/www --exec babel-node --presets babel-preset-env",
    "local": "set NODE_ENV=local nodemon ./bin/www --exec babel-node --presets babel-preset-env",
    "development": "NODE_ENV=development node ./bin/www --exec babel-node --presets babel-preset-env",
    "production": "NODE_ENV=production node ./bin/www --exec babel-node --presets babel-preset-env",
    "frontend": "cd ../frontend && npm start",
    "backendfrontenddev": "concurrently \"npm run start\" \"npm run frontend\""
  },
  "keywords": [
    "node.js"
  ],
  "author": "nextinnovation labs",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.28",
    "morgan": "^1.10.0",
    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rand-token": "^1.0.1",
    "sequelize": "^6.3.4",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.1",
    "uuid": "^8.3.2"
  }
}
